{
    "mode": "typescript",
    "recurseDepth": 10,
    "markdown": {
        "hardwrap": true,
        "idInHeadings": false
    },
    "menu": {
        "API Documentation": {
            "href": "",
            "target": "_blank",
            "class": "menu-item",
            "id": "repository"
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./dist/doc",
        "recurse": true,
        "template": "node_modules/jsdoc"
    },
    "plugins": [],
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "node_modules",
            "src/types"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "tagNamePreference": {
        "virtual": "virtual",
        "augments": "extends"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        },
        "scripts": [],
        "favicon": "",
        "css": []
    }
}