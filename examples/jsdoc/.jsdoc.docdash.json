{
    "mode": "typescript",
    "sourceType": "module",
    "recurseDepth": 10,
    "markdown": {
        "hardwrap": true,
        "idInHeadings": false
    },
    "menu": {
        "API Documentation": {
            "href": "",
            "target": "_blank",
            "class": "menu-item",
            "id": "repository"
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./dist/doc/api",
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "node_modules",
            "src/types"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "tagNamePreference": {
        "virtual": "virtual",
        "augments": "extends"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        },
        "scripts": [],
        "favicon": "",
        "css": []
    },
    "docdash": {
        "static": true,
        "sort": true,
        "sectionOrder": [
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "search": true,
        "collapse": true,
        "wrap": true,
        "typedefs": true,
        "navLevel": 3,
        "private": false,
        "removeQuotes": "trim",
        "scripts": [],
        "menu": {
            "Donate": {
                "href": "https://github.com/spmeesseman/webpack-wrap",
                "target": "_blank",
                "class": "menu-item",
                "id": "website_link"
            },
            "Repository": {
                "href": "https://github.com/spmeesseman/webpack-wrap",
                "target": "_blank",
                "class": "menu-item",
                "id": "website_link"
            }
        },
        "scopeInOutputPath": false,
        "nameInOutputPath": false,
        "versionInOutputPath": false
    }
}