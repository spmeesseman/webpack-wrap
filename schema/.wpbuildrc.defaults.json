{
    "$schema": "https://app1.spmeesseman.com/res/app/wpbuild/v0.0.1/.wpbuildrc.schema.json",
    "name": "wpw-app",
    "displayName": "Webpack Wrap Application",
    "detailedDisplayName": "Task Explorer VSCode Extension",
    "publicInfoProject": true,
    "builds": [],
    "development": {
        "builds": []
    },
    "production": {
        "builds": []
    },
    "test": {
        "builds": []
    },
    "log": {
        "level": 2,
        "valueMaxLineLength": 100,
        "pad": {
            "base": 0,
            "envTag": 22,
            "value": 45,
            "uploadFileName": 50
        },
        "colors": {
            "default": "grey",
            "buildBracket": "blue",
            "buildText": "white",
            "infoIcon": "magenta",
            "valueStar": "cyan",
            "valueStarText": "white",
            "tagBracket": "blue",
            "tagText": "white",
            "uploadSymbol": "yellow"
        }
    },
    "paths": {
        "dist": "dist",
        "src": "src"
    },
    "exports": {
        "cache": true,
        "devtool": true,
        "experiments": true,
        "externals": true,
        "ignorewarnings": true,
        "minification": false,
        "optimization": true,
        "stats": true,
        "watch": true
    },
    "plugins": {
        "analyze": {
            "circular": true,
            "analyzer": false,
            "visualizer": false
        },
        "banner": false,
        "ignore": true,
        "istanbul": false,
        "licensefiles": false,
        "loghooks": true,
        "optimization": true,
        "progress": false,
        "scm": false,
        "sourcemaps": true,
        "upload": false,
        "vendormod": true
    },
    "vscode": {
        "type": "none"
    }
}