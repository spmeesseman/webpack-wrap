{
    "$schema": "https://app1.spmeesseman.com/res/app/wpbuild/v0.0.1/schema/.wpbuildrc.schema.json",
    "builds": [],
    "development": {
        "builds": []
    },
    "production": {
        "builds": [],
        "options": {
            "analyze":true,
            "wait": {
                "abc": true
            }
        }
    },
    "test": {
        "builds": []
    },
    "log": {
        "level": 2,
        "valueMaxLineLength": 100,
        "pad": {
            "base": 0,
            "envTag": 22,
            "value": 45,
            "uploadFileName": 50
        },
        "colors": {
            "default": "grey",
            "buildBracket": "blue",
            "buildText": "white",
            "infoIcon": "magenta",
            "valueStar": "cyan",
            "valueStarText": "white",
            "tagBracket": "blue",
            "tagText": "white",
            "uploadSymbol": "yellow"
        }
    },
    "paths": {
        "dist": "dist",
        "src": "src"
    },
    "options": {
        "analyze": {
            "circular": false,
            "analyzer": false,
            "visualizer": false
        },
        "banner": false,
        "cache": false,
        "copy": false,
        "devtool": true,
        "experiments": true,
        "externals": true,
        "ignore": true,
        "ignorewarnings": false,
        "istanbul": false,
        "jsdoc": false,
        "licensefiles": false,
        "loghooks": true,
        "minification": false,
        "optimization": true,
        "progress": false,
        "sourcemaps": false,
        "watch": false
    },
    "vscode": {
        "type": "none"
    }
}