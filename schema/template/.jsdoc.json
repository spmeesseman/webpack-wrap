{
    "mode": "typescript",
    "recurseDepth": 10,
    "sourceType": "module",
    "markdown": {
        "hardwrap": true,
        "idInHeadings": true
    },
    "menu": {
        "API Documentation": {
            "href": "",
            "target": "_blank",
            "class": "menu-item",
            "id": "repository"
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./dist/doc/api",
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "dark",
            "displayModuleHeader": true,
            "includeFilesListInHomepage": true,
            "removeQuotes": "trim",
            "add_scripts": "",
            "search": true,
            "sort": true,
            "scopeInOutputPath": false,
            "nameInOutputPath": false,
            "versionInOutputPath": false,
            "homepageTitle": "Webpack-Wrap",
            "favicon": "res/img/favicon.ico",
            "title": "Webpack-Wrap",
            "add_script_path": [],
            "sections": [
                "Classes",
                "Modules",
                "Externals",
                "Events",
                "Namespaces",
                "Mixins",
                "Tutorials",
                "Interfaces"
            ],
            "codepen": {
                "enable_for": [
                    "examples"
                ],
                "options": {
                  "js_external": "https://code.jquery.com/jquery-3.6.0.min.js",
                  "js_pre_processor": "babel"
                }
            },
            "menu": [
                {
                    "title": "Donate",
                    "href": "https://github.com/spmeesseman/webpack-wrap",
                    "target": "_blank",
                    "class": "menu-item",
                    "id": "donate_link"
                },
                {
                    "title": "Repository",
                    "href": "https://github.com/spmeesseman/webpack-wrap",
                    "target": "_blank",
                    "class": "menu-item",
                    "id": "repo_link"
                }
            ],
            "meta": [
                {
                  "name": "author",
                  "content": "Scott Meesseman"
                },
                {
                  "name": "description",
                  "content": "Webpack-Wrap - lots and lots of Webpack functionality in one package"
                }
            ]
        }
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [],
        "exclude": [
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "tagNamePreference": {
        "virtual": "virtual",
        "augments": "extends"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        },
        "scripts": [],
        "favicon": "",
        "css": []
    }
}