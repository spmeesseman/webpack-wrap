{
    "name": "@spmeesseman/webpack-wrap",
    "version": "0.0.1",
    "displayName": "Webpack Build Wrapper",
    "description": "Webpack Build Wrapper - Webpack build made easy, adapt new projects with ease",
    "publisher": "spmeesseman",
    "license": "MIT",
    "homepage": "https://github.com/spmeesseman/webpack-wrap/blob/master/README.md",
    "author": {
        "name": "Scott Meesseman",
        "email": "spmeesseman@v.com"
    },
    "bugs": {
        "email": "spmeesseman@gmail.com",
        "url": "https://github.com/spmeesseman/webpack-wrap/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spmeesseman/webpack-wrap.git"
    },
    "funding": {
        "type": "paypal",
        "url": "https://www.paypal.com/donate/?hosted_button_id=VNYX9PP5ZT5F8"
    },
    "scripts": {
        "build-all-development": "webpack --mode development --config ./webpack.config.js",
        "build-all-production": "webpack --mode production --config ./webpack.config.js",
        "build-all-test": "webpack --mode none --env mode=test --config ./webpack.config.js",
        "build-app-development": "webpack --env build=app --mode development --config ./webpack.config.js",
        "build-app-development:analyze": "webpack --env build=app --mode development --env analyze --config ./webpack.config.js && npm run view-statistics",
        "build-app-production": "webpack --env build=app --mode production --config ./webpack.config.js",
        "build-app-production:analyze": "webpack --env build=app --mode production --env analyze --config ./webpack.config.js && npm run view-statistics",
        "build-app-test": "webpack --env build=app --mode none --env mode=test --config ./webpack.config.js",
        "build-app-test:analyze": "webpack --env build=app --mode none --env mode=test --env analyze --config ./webpack.config.js && npm run view-statistics",
        "build-docs": "webpack --env build=docs --mode production --config ./webpack.config.js",
        "build-docs:test_jsdoc": "jsdoc --destination res/docs/api --configure .jsdoc.json --recurse ./src",
        "build-examples": "node script/create-examples.js",
        "build-schema": "npm run schema-build-types",
        "build-types": "webpack --env build=types --mode none --env mode=test --config ./webpack.config.js",
        "build-test_json2ts": "json2ts -f --unreachableDefinitions --style.tabWidth 4 --no-additionalProperties -i ./schema/spm.schema.wpw.json -o types/rc.tmp.d.ts",
        "clean-all_w_schema": "npm run clean-all && npm run clean-schema",
        "clean-all": "npm run clean-app && npm run clean-docs && npm run clean-examples && rimraf dist && rimraf node_modules/.cache/wpwrap",
        "clean-app": "rimraf dist/webpack-wrap.*js && rimraf dist/vendor.*js && rimraf dist/runtime.*js && rimraf node_modules/.cache/wpwrap/webpack/webpack-wrap*",
        "clean-docs": "rimraf dist/docs && rimraf node_modules/.cache/wpwrap/webpack/docs*",
        "clean-examples": "rimraf examples/vscode && rimraf dist/examples",
        "clean-types": "rimraf dist/types && rimraf dist/types.d.ts && rimraf node_modules/.cache/wpwrap/tsconfig.types.tsbuildinfo && rimraf node_modules/.cache/wpwrap/webpack/types*",
        "clean-schema": "rimraf src/types/rc.ts && rimraf src/types/constants.js",
        "clean-wpcache": "rimraf node_modules/.cache/wpwrap/webpack",
        "test-run": "echo TODO",
        "test-run_schema_validation": "echo TODO",
        "test-js_tsc_check": "tsc --target es2020 --moduleResolution node --allowSyntheticDefaultImports --noEmit --skipLibCheck ./src/types/index.ts && tsc --target es2020 --allowSyntheticDefaultImports --moduleResolution node --noEmit --skipLibCheck --allowJs ./src/core/rc.js",
        "rebuild-all-development": "rpm run clean-all && npm run build-all-development",
        "rebuild-all-production": "rpm run clean-all && npm run build-all-production",
        "rebuild-all-test": "rpm run clean-all && npm run build-all-test",
        "rebuild-docs": "rpm run clean-docs && npm run build-docs",
        "rebuild-app-development": "rpm run clean-all && npm run build-app-development",
        "rebuild-app-production": "rpm run clean-all && npm run build-app-production",
        "rebuild-app-test": "rpm run clean-all && npm run build-app-test",
        "rebuild-schema": "rpm run clean-schema && npm run build-schema",
        "rebuild-types": "rpm run clean-types && npm run build-types",
        "schema-upload": "node script/upload-rc-schema.js",
        "schema-build:types": "node script/generate-rc-types.js",
        "schema-build:types_and_upload": "npm run build-types-schema && npm run build-types && npm run upload_schema",
        "view-statistics": "cmd /c \"C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe\" file://c:/Projects/vscode-taskexplorer/.coverage/visualizer.html"
    },
    "files": [
        "bin",
        "examples",
        "res",
        "schema",
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "webpack",
        "javascript",
        "typescript",
        "wrapper",
        "helper"
    ],
    "main": "./dist/webpack-wrap.js",
    "bin": {
        "wpw": "./bin/wpwrap.js",
        "wpwrap": "./bin/wpwrap.js",
        "webpack-wrap": "./bin/wpwrap.js"
    },
    "engines": {
        "node": ">=14.16.0"
    },
    "peerDependencies": {
        "typescript": ">3.6.0",
        "webpack": ">5.60.0",
        "webpack-cli": ">5.1.0"
    },
    "optionalDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "circular-dependency-plugin": "^5.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-asset-in-memory-webpack-plugin": "^4.0.1",
        "copy-webpack-plugin": "^11.0.0",
        "csp-html-webpack-plugin": "^5.1.0",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "esbuild": "^0.17.8",
        "esbuild-loader": "^2.21.0",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "image-minimizer-webpack-plugin": "^3.8.1",
        "jsdoc": "^4.0.2",
        "jsdoc-api": "^8.0.0",
        "mini-css-extract-plugin": "^2.7.2",
        "sass": "^1.58.0",
        "sass-loader": "^13.2.0",
        "string-replace-loader": "^3.1.0",
        "ts-loader": "^8.0.17",
        "tsc-alias": "^1.8.2",
        "tsconfig-paths": "^4.1.2",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-node-externals": "^3.0.0",
        "webpack-visualizer-plugin2": "^1.0.0"
    },
    "devDependencies": {
        "@spmeesseman/eslint-plugin": "^1.0.0",
        "@types/circular-dependency-plugin": "^5.0.5",
        "@types/node": "^16.18.34",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/eslint-plugin-tslint": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^7.32.0",
        "eslint-plugin-anti-trojan-source": "^1.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^46.4.4",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^3.3.1",
        "json-schema-to-typescript": "^13.0.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.5",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@spmeesseman/arg-parser": "^1.2.0",
        "@spmeesseman/type-utils": "file:../type-utils",
        "babel-loader": "^9.1.2",
        "capture-console": "^1.0.1",
        "dts-bundle": "^0.7.3",
        "glob": "^10.3.3",
        "gradient-string": "^2.0.2",
        "json5": "^2.2.2",
        "loader-utils": "^3.2.1",
        "minimatch": "^5.1.2",
        "node-fetch": "^3.3.1",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "prompt": "^1.3.0",
        "schema-utils": "^4.2.0"
    }
}
