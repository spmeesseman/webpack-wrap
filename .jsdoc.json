{
    "mode": "typescript",
    "recurseDepth": 10,
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    },
    "menu":{
      "Webpack-Wrap Documentation": {
        "href":"https://app1.spmeesseman.com/res/docs/",
        "target":"_blank",
        "class":"menu-item",
        "id":"repository"
      }
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./dist/doc/api",
      "recurse": true,
      "template": "node_modules/clean-jsdoc-theme",
      "theme_opts": {
          "default_theme": "dark"
      }
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "node_modules",
            "src/types",
            "typedefs.js"
        ],
        "includePattern": "src[\\\\\\/].+\\.(m|c)?js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "tagNamePreference": {
        "virtual": "virtual",
        "augments": "extends"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
          "outputSourceFiles": true,
          "includeDate": false
        },
        "scripts": [],
        "favicon": "",
        "css": []
    },
    "docdash": {
        "static": true,
        "sort": true,
        "sectionOrder": [
            "Classes",
            "Modules",
            "Externals",
            "Events",
            "Namespaces",
            "Mixins",
            "Tutorials",
            "Interfaces"
        ],
        "disqus": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "search": true,
        "collapse": true,
        "wrap": true,
        "typedefs": true,
        "navLevel": 3,
        "private": false,
        "removeQuotes": "trim",
        "scripts": [],
        "menu": {
            "Donate": {
                "href":"https://github.com/spmeesseman/webpack-wrap",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            },
            "Repository": {
                "href":"https://github.com/spmeesseman/webpack-wrap",
                "target":"_blank",
                "class":"menu-item",
                "id":"website_link"
            }
        },
        "scopeInOutputPath": false,
        "nameInOutputPath": false,
        "versionInOutputPath": false
    }
}